{
  "name": "url-naong",
  "version": "1.0.0",
  "description": "url builder library for javascript, typescript",
  "scripts": {
    "test": "just --config ./.config/just.config.ts test",
    "coverage": "just --config ./.config/just.config.ts coverage",
    "clean": "just --config ./.config/just.config.ts clean",
    "pub:prod": "just --config ./.config/just.config.ts pub:prod",
    "pub:dev": "just --config ./.config/just.config.ts pub:dev",
    "build": "just --config ./.config/just.config.ts build",
    "webpack:prod": "just --config ./.config/just.config.ts webpack:prod",
    "webpack:dev": "just --config ./.config/just.config.ts webpack:dev",
    "dts-bundle": "just --config ./.config/just.config.ts dts-bundle",
    "lint": "just --config ./.config/just.config.ts lint",
    "dev": "ts-node ./src/cli.ts",
    "debug": "node -r ts-node/register --inspect-brk --nolazy src/cli.ts"
  },
  "keywords": [
    "url",
    "uri",
    "build",
    "builder",
    "querystring",
    "qs",
    "query",
    "stringify",
    "route",
    "regexp"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/url-naong.git"
  },
  "author": "ByungJoon Lee",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/imjuni/url-naong/issues"
  },
  "homepage": "https://github.com/imjuni/url-naong#readme",
  "dependencies": {
    "path-to-regexp": "^6.2.1",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.5",
    "@types/prettier": "^2.6.3",
    "@types/qs": "^6.9.7",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/eslint-plugin-tslint": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "cross-env": "^7.0.3",
    "dts-bundle-generator": "^6.12.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "just-scripts": "^2.0.0",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^28.0.6",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}